{# What displays on /courses-and-events when no results come up in the search. 
The text that appears lives in a custom block found here: /block/86 #}

{{ attach_library('coffey/courses-events-view') }}

{% if header %}
  <header>
    {{ header }}
  </header>
{% endif %}

{% if rows | length %}
<div class="no-results-events">
  {{ rows }}
</div>

{% endif %}

{% if footer %}
  <footer>
    {{ footer }}
  </footer>
{% endif %}